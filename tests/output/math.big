func main(): int32 {
  println("%d", 1 + 2);
  println("%d", 2 - 1);
  println("%d", 21 * 2);
  println("%d", 42 / 2);
  println("%d", 1 + 2 * 3);
  println("%d", 4 * (2 + 3));

  var value: int32 = 42;

  value += 12;
  println("%d", value);

  value -= 24;
  println("%d", value);

  value *= 2;
  println("%d", value);

  value /= 6;
  println("%d", value);

  value = -value;
  println("%d", value);

  return 0;
}